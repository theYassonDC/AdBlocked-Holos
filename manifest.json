{
    "name": "AdBlock Holos",
    "version": "0.0.2.0",
    "manifest_version": 3,
    "description": "Extension con el uso y finalidad de bloquear publicidad de los habbo holos. - Version beta",
    "author": "JuandaC",
    "icons": {
        "128": "icons/block.png"
    },
    "action": {
        "default_popup": "popup/popup.html"
    },
    "content_scripts": [
        {
            "matches": [
                "https://kubbo.city/*",
                "https://hgalaxy.net/*",
                "https://www.hobba.tv/*"
            ],
            "js": [
                "scripts/blocker.main.js"
            ]
        }
    ]
}